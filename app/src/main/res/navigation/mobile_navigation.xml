<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_server">



    <fragment
        android:id="@+id/navigation_server"
        android:name="com.example.bluetooth.ui.server.ServerFragment"
        android:label="@string/title_server"
        tools:layout="@layout/fragment_server" />

    <fragment
        android:id="@+id/navigation_client"
        android:name="com.example.bluetooth.ui.client.ClientFragment"
        android:label="@string/title_client"
        tools:layout="@layout/fragment_client" />

    <fragment
        android:id="@+id/navigation_peripheral"
        android:name="com.example.bluetooth.ui.peripheral.PeripheralFragment"
        android:label="@string/title_peripheral"
        tools:layout="@layout/fragment_peripheral" />

    <fragment
        android:id="@+id/navigation_central"
        android:name="com.example.bluetooth.ui.central.CentralFragment"
        android:label="@string/title_central"
        tools:layout="@layout/fragment_central" />
</navigation>